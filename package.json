{
  "name": "nodets",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "commonjs",
  "volta": {
    "node": "17.9.1",
    "yarn": "1.22.19"
  },
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "db:console": "docker compose exec postgres psql -h localhost -U postgres -d webapp_dev",
    "db:migrate": "knex migrate:latest",
    "docker:db:migrate": "docker compose run backend yarn db:migrate",
    "rebuild:backend": "docker compose build backend && docker compose rm --stop --yes backend && docker compose up -d backend",
    "start": "node dist/index.js",
    "start:docker": "yarn build && node --inspect=0.0.0.0 dist/index.js"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.9.0",
    "@prisma/client": "^4.0.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "express": "^4.18.1",
    "knex": "^2.1.0",
    "morgan": "^1.10.0",
    "nanoid": "3.3",
    "pg": "^8.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0",
    "router": "^1.3.7",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.213",
    "@tsconfig/node16": "^1.0.3",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "prisma": "^4.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1"
  }
}
