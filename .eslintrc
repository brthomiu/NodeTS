{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    
    "rules": {
        "@typescript-eslint/no-var-requires": "off",
        "no-debugger": "off"
    },

    "env": {
        "browser": true,
        "node": true,
        "es6": true
    }
}